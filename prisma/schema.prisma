// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Quote {
  id             Int    @id @default(autoincrement())
  name           String
  destination    String
  from           String
  departDate     String
  returnDate     String
  people         Int
  transportation String
}
